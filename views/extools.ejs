<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">       
    <title>reguser</title>
    <link rel="stylesheet" type="text/css" href="/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="/css/style.css" />
  </head>

  <body ng-app="extools">
    <div ng-controller="exCtrl" id="container" class="container">
      <form class="form-signin" ng-submit="postReq()" role="form" method="post" novalidate>
        <h2 class="form-signin-heading">注册</h2>
        <textarea name="txtRequest" id="txtRequest" cols="60" rows="6" ng-model="txtReq">
        </textarea>
        <p></p>
        <textarea name="txtReturn" id="txtReturn" cols="60" rows="20" ng-model="txtReturn">
        </textarea>
        <button id="btnSub" class="btn btn-lg btn-primary" type="submit">发送</button>
      </form>                 
    </div> 
  </body>
</html>
<script src="/js/angular1.3.min.js" type="text/javascript"></script>
<script src="/lab/angExTools.js" type="text/javascript"></script>
